{
  "name": "interns-projects",
  "version": "1.0.0",
  "description": "Goal of the project is to provide the platform to write algorithms, tests and get markup knowledge",
  "scripts": {
    "test": "jest",
    "lint": "./node_modules/.bin/eslint tasks/**/*.js",
    "lint:fix": "./node_modules/.bin/eslint --fix tasks/**/*.js",
    "tdd": "jest --watch",
    "watch:sass": "node-sass -w css-practice/task2/workspace/scss/ -o css-practice/task2/workspace/css/",
    "build:sass": "node-sass css-practice/task2/workspace/scss/ -o css-practice/task2/workspace/css/"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.10.0",
    "husky": "^1.1.2",
    "jest": "^21.1.0",
    "node-sass": "^4.10.0",
    "regenerator-runtime": "^0.11.0"
  },
  "dependencies": {
    "sass": "^1.14.3"
  }
}
